{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch via NPM",
      "cwd": "${workspaceRoot}",
      "runtimeExecutable": "npm.cmd",
      "runtimeArgs": [
        "start",
        "--inspect"
      ],
      "timeout": 10000
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Electron",
      "runtimeExecutable": "${workspaceRoot}\\node_modules\\.bin\\electron",
      "cwd":"${wordspaceRoot}",
      "runtimeArgs": [
        "${workspaceRoot}\\src\\electron-starter.js",
        "--remote-debugging-port=9876",
        "--enable-logging",
      ],
      "port": 9876
    }
  ]
}